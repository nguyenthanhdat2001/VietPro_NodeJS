<% if(pages.length > 1) { %>
<nav aria-label="Page navigation example">
    <ul class="pagination">
        <% if(page > 1) { %> 
            <li class="page-item"><a class="page-link" href="<%= path %>?page=<%= page - 1 %>">&laquo;</a></li>
        <% } %>     
        <% for(pageNum of pages) { %> 
            <% if(pageNum === '...') {%> 
                <li class="page-item"><span class="page-link"><%= pageNum%></span></li>
            <% }else{ %> 
                <li class="page-item <%=pageNum===page ? 'active' : '' %>">
                    <a class="page-link" href="<%= path %>?page=<%= pageNum %>"><%= pageNum %></a></li>
            <% } %>
        <% } %>
        <% if(page < totalPages) { %>
            <li class="page-item"><a class="page-link" href="<%= path %>?page=<%= page + 1 %>">&raquo;</a></li>
        <% } %>
    </ul>
</nav>
<% } %>